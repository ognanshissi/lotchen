  "scripts": {
    "start": "npx nx run Lotchen:serve:development",
    "api:start": "npx nx run lotchen-api:serve",
    "start:api": "node dist/apps/lotchen-api/main.js",
    "start:voice-api": "node dist/apps/twilio-voice-webhook-api/main.js",
    "start:all": "npm run start && npm run api:start",
    "build": "npx nx run Lotchen:build:production",
    "build:voice-api:prod": "npx nx build twilio-voice-webhook-api --prod",
    "ssr:dev": "npx nx run Lotchen:serve-ssr",
    "ui:watch": "npx nx run ui:build:development --watch",
    "ui:build": "npx nx run ui:build:development --verbose",
    "doc": "npx @compodoc/compodoc -p libs/lotchen-api/identity-provider/tsconfig.json -s",
    "openapi-generator": "npx @openapitools/openapi-generator-cli generate  --generator-key lotchen --verbose",
    "prepare": "husky",
    "api": "pm2 start dist/apps/lotchen-api/main.js",
    "heroku-postbuild": "npx nx build $PROJECT_NAME --prod"
  },
