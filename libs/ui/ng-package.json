{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/libs/ui",
  "assets": [
    {
      "glob": "**/*",
      "input": "styles/src/themes",
      "output": "styles/themes"
    },
    {
      "glob": "**/*",
      "input": "icon/src/assets",
      "output": "icon/assets"
    }
  ],
  "lib": {
    "entryFile": "src/index.ts"
  }
}
